        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.bytts.dao.BookShelfDao">

    <select id="selectBookShelfByTag" resultType="cn.bytts.domain.BookShelf">
        select * from t_book_shelf where tag = #{tag}
